    <file src="(Stage)\Bin\net46\KornSW.DynamicRepository.dll" target="lib/net46" />
    <file src="(Stage)\Bin\net46\KornSW.DynamicRepository.xml" target="lib/net46" />
    <file src="(Stage)\Bin\net46\KornSW.DynamicRepository.pdb" target="lib/net46" />
    <!-- .NET 5.0 -->
    <file src="(Stage)\Bin\net5.0\KornSW.DynamicRepository.dll" target="lib/net5.0" />
