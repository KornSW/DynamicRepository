<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>KornSW.DynamicRepository</id>
    <version>0.1.0-localbuild</version>
    <title>KornSW.DynamicRepository</title>
    <authors>KornSW</authors>
    <owners>KornSW</owners>
    <copyright>KornSW</copyright>
    <description>provides functionality to supply repositories on a generic way without writing too much redundant code</description>
    <tags>EntityFramework Linq Repository</tags>
    <!-- https://spdx.org/licenses/ -->
    <license type="expression">GPL-3.0-only</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <icon>PackageIcon.png</icon>
    <projectUrl>https://github.com/KornSW/DynamicRepository</projectUrl>
    <dependencies>
      <group targetFramework=".NETFramework4.6">
